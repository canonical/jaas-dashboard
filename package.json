{
  "name": "juju-dashboard",
  "version": "0.13.3",
  "description": "A dashboard for Juju and JAAS (Juju as a service)",
  "bugs": {
    "url": "https://github.com/canonical/juju-dashboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canonical/juju-dashboard.git"
  },
  "type": "module",
  "license": "LGPL-3.0",
  "author": {
    "name": "Canonical Webteam",
    "email": "webteam@canonical.com"
  },
  "main": "index.js",
  "scripts": {
    "build": "vite build && yarn run generate-version-file",
    "clean": "./scripts/clean-files",
    "eslint-check": "eslint src/",
    "generate-release-tarball": "yarn run build && ./scripts/generate-release-tarball",
    "generate-version-file": "./scripts/generate-version-file",
    "lint": "npmPkgJsonLint . && yarn run eslint-check && yarn run prettier-check && yarn run stylelint-check",
    "prettier-check": "prettier --check 'src/**/*'",
    "serve": "yarn run start",
    "start": "vite",
    "stylelint-check": "stylelint 'src/**/*.scss'",
    "stylelint-fix": "stylelint --fix 'src/**/*.scss'",
    "test": "vitest",
    "typescript-build": "tsc -b"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@canonical/jujulib": "7.0.0",
    "@canonical/macaroon-bakery": "1.3.2",
    "@canonical/react-components": "0.59.0",
    "@reduxjs/toolkit": "2.2.4",
    "@sentry/browser": "7.114.0",
    "ansi-to-html": "0.7.2",
    "async-limiter": "2.0.0",
    "classnames": "2.5.1",
    "clone-deep": "4.0.1",
    "d3": "7.9.0",
    "date-fns": "3.6.0",
    "dompurify": "3.1.3",
    "formik": "2.4.6",
    "framer-motion": "11.1.9",
    "fuse.js": "7.0.0",
    "lodash.isequal": "4.5.0",
    "lodash.mergewith": "4.6.2",
    "prism-react-renderer": "2.3.1",
    "prismjs": "1.29.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-ga": "3.3.1",
    "react-hot-toast": "2.4.1",
    "react-json-tree": "0.19.0",
    "react-redux": "9.1.2",
    "react-router-dom": "6.23.0",
    "react-useportal": "1.0.19",
    "redux": "5.0.1",
    "vanilla-framework": "4.14.0",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/classnames": "2.3.1",
    "@types/clone-deep": "4.0.4",
    "@types/d3": "7.4.3",
    "@types/dompurify": "3.0.5",
    "@types/lodash.isequal": "4.5.8",
    "@types/lodash.mergewith": "4.6.9",
    "@types/node": "18.19.33",
    "@types/prismjs": "1.26.4",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/react-table": "7.7.20",
    "@types/redux-mock-store": "1.0.6",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
